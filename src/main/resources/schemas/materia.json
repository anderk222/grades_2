{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://example.com/materia.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "descripcion"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                0
            ]
        },
        "descripcion": {
            "type": "string",
            "default": "",
            "title": "The descripcion Schema",
            "maxLength":100,
            "minLength":2,
            "examples": [
                "Matematicas"
            ]
        }
    },
    "examples": [{
        "id": 0,
        "descripcion": "Matematicas"
    }]
}